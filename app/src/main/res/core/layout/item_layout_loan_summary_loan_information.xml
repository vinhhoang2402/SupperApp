<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@color/transparent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <androidx.cardview.widget.CardView
        app:cardCornerRadius="0dp"
        app:cardElevation="4dp"
        android:id="@+id/cvTop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clLoanProfile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_rectangle_29000000"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleLoanProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_16dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                android:text="@string/text_loan_profile"
                android:textAllCaps="true"
                style="@style/StyleTextViewMedium.Medium.6D30AB"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleIDLoanProfile"
                style="@style/StyleTextViewMedium.Normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                android:text="@string/text_loan_profile_id"
                android:textColor="@color/color_a7a1a1"
                android:textAllCaps="true"
                app:layout_constraintBottom_toTopOf="@id/tvIDLoanProfile"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvTitleLoanProfile" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvIDLoanProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_6dp"
                style="@style/StyleTextViewMedium.Medium"
                android:textColor="@color/color_4a3bcc"
                android:textAllCaps="true"
                android:text="@string/text_id12450"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                app:layout_constraintTop_toBottomOf="@id/tvTitleIDLoanProfile"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toTopOf="@id/tvTitleBorrower"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivBank"
                android:layout_width="@dimen/spacing_150"
                android:layout_height="@dimen/spacing_61dp"
                android:padding="@dimen/spacing_4dp"
                android:src="@drawable/img_bank_techcom"
                android:background="@drawable/bg_rectangle_fcfcfc_ebe0f6"
                android:layout_marginEnd="@dimen/spacing_14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/tvTitleIDLoanProfile"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintTop_toTopOf="@id/tvTitleIDLoanProfile"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleBorrower"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_borrower"
                android:textColor="@color/color_a7a1a1"
                android:textAllCaps="true"
                app:layout_constraintTop_toBottomOf="@id/tvIDLoanProfile"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvBorrower"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:text="@string/text_person_name"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                app:layout_constraintTop_toBottomOf="@id/tvTitleBorrower"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitlePhoneNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_16dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_telephone_file"
                android:textAllCaps="true"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvBorrower"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvPhoneNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_16dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:text="@string/text_telephone_file_online"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                app:layout_constraintTop_toBottomOf="@id/tvTitlePhoneNumber"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_street_file_online"
                android:textAllCaps="true"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvPhoneNumber"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvAddress"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                android:text="@string/text_address"
                app:layout_constraintTop_toBottomOf="@id/tvTitleAddress"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_email"
                android:textAllCaps="true"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvAddress"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvEmail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:layout_marginBottom="@dimen/spacing_26dp"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                android:text="@string/text_use_email_file_online"
                app:layout_constraintTop_toBottomOf="@id/tvTitleEmail"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintBottom_toBottomOf="parent"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/cvMid"
        app:cardCornerRadius="0dp"
        app:cardElevation="4dp"
        android:layout_marginTop="@dimen/spacing_16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cvTop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clSalePolicy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_rectangle_29000000"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cvTop">
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleSalePolicy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_16dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                android:text="@string/text_sales_policy"
                android:textAllCaps="true"
                style="@style/StyleTextViewMedium.Medium.6D30AB"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleSalePolicyType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_sale_policy_type"
                android:textAllCaps="true"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvTitleSalePolicy"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvSalePolicyType"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:text="@string/text_standard_policy"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                app:layout_constraintTop_toBottomOf="@id/tvTitleSalePolicyType"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleDeposit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_deposit_vnd"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvSalePolicyType"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvDeposit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                android:text="@string/text_pay_100000000"
                app:layout_constraintTop_toBottomOf="@id/tvTitleDeposit"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleNumberPayment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_number_of_payment"
                android:textAllCaps="true"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvDeposit"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvNumberPayment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                android:text="@string/text_9"
                app:layout_constraintTop_toBottomOf="@id/tvTitleNumberPayment"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleTotalMoney"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_title_total_money_vnd_caplock"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvNumberPayment"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTotalMoney"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:layout_marginBottom="@dimen/spacing_22dp"
                android:text="@string/text_3_000_000_000"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                app:layout_constraintTop_toBottomOf="@id/tvTitleTotalMoney"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintBottom_toBottomOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:cardCornerRadius="0dp"
        app:cardElevation="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="@dimen/spacing_10dp"
        android:layout_marginTop="@dimen/spacing_16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cvMid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clLoanPolicy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_rectangle_29000000"
            android:elevation="6dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cvMid"
            app:layout_constraintBottom_toBottomOf="parent">
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleLoanPolicy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_16dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                android:text="Loan Policy"
                android:textAllCaps="true"
                style="@style/StyleTextViewMedium.Medium.6D30AB"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleMethodLoan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_policy_loan_loan_method"
                android:textAllCaps="true"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvTitleLoanPolicy"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvMethodLoan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:text="@string/title_floating_loan"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                app:layout_constraintTop_toBottomOf="@id/tvTitleMethodLoan"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleAmountLoan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_loan_amount_vnd"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvMethodLoan"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvAmountLoan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                android:text="@string/text_hint_amount_loan"
                app:layout_constraintTop_toBottomOf="@id/tvTitleAmountLoan"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleTenor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:textColor="@color/color_a7a1a1"
                android:text="@string/text_tenor_year"
                app:layout_constraintTop_toBottomOf="@id/tvAmountLoan"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTenor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                android:text="@string/text_10"
                app:layout_constraintTop_toBottomOf="@id/tvTitleTenor"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTitleTotalPayment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_20dp"
                style="@style/StyleTextViewMedium.Normal"
                android:text="@string/text_total_price_vnd"
                android:textColor="@color/color_a7a1a1"
                app:layout_constraintTop_toBottomOf="@id/tvTenor"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTotalPayment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/spacing_14dp"
                android:layout_marginTop="@dimen/spacing_4dp"
                android:layout_marginEnd="@dimen/spacing_14dp"
                android:layout_marginBottom="@dimen/spacing_22dp"
                android:text="@string/text_595_729_154"
                android:fontFamily="@font/roboto_bold"
                android:textSize="@dimen/text_14sp"
                android:textColor="@color/color_241f1f"
                app:layout_constraintTop_toBottomOf="@id/tvTitleTotalPayment"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintBottom_toBottomOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>
