<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_marginEnd="@dimen/spacing_14dp"
    android:gravity="center"
    android:id="@+id/ctTypeProject"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bg_rectangle_c4_ffffff_stroke_a4a4a4"
    android:layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvTypeStatus"
        style="@style/StyleTextView.Normal.Silver"
        android:background="?attr/selectableItemBackground"
        android:paddingStart="@dimen/spacing_12dp"
        android:paddingTop="@dimen/spacing_6dp"
        android:paddingEnd="@dimen/spacing_12dp"
        android:paddingBottom="@dimen/spacing_6dp"
        android:text="@string/text_status_finish"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</androidx.constraintlayout.widget.ConstraintLayout>